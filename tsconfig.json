{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@layout/*": ["./src/layout/*"],
      "@lib/*": ["./src/lib/*"],
      "@packages/*": ["./src/packages/*"],
      "@screens/*": ["./src/screens/*"],
      "@stacks/*": ["./src/stacks/*"],
      "@themes/*": ["./src/themes/*"]
    }
  }
}
